	date-added = {2023-09-04 17:40:45 -0700},
	date-modified = {2023-09-04 17:40:45 -0700},
	doi = {10.1007/BF00665652},
	group = {Thermalization; Physics 521},
	journal = {Foundations of Physics Letters},
	number = {4},
	pages = {327 -- 337},
	title = {Classical mechanics and entropy},
	usedby = {mforbes},
	volume = {6},
	year = {1993},
	abstract = {This note addresses a problem of nineteenth century applied mathematics---is it possible in the context of Hamiltonian mechanics to define a function S of the generalized coordinates and momenta which is monotonically increasing along orbits? The question is of interest, because, for a sytem not in thermodynamic equilibrium, entropy should increase strictly monotonically along an orbit, and a negative answer implies that mechanical principles different from those of Hamiltonian mechanics must be introduced to explain thermodynamics. This note answers the question rigorously for Hamiltonian systems confined to an invariant region of finite volume in phase space; it is not possible to define a continuous function which increases monotonically along orbits. An appendix gives a translation of an 1889 paper of Poincar\'e addressing the same issue.},
	bdsk-file-1 = {YnBsaXN0MDDRAQJccmVsYXRpdmVQYXRoXxBKLi4vLi4vLi4vLi4vLi4vLi4vLi4vZGF0YS9QYXBlcnMvQmliRGVzay9PbHNlbi8xOTkzXzEwLjEwMDctQkYwMDY2NTY1Mi5wZGYICxgAAAAAAAABAQAAAAAAAAADAAAAAAAAAAAAAAAAAAAAZQ==},
